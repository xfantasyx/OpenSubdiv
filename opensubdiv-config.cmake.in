@PACKAGE_INIT@

# set_and_check(OpenSubdiv_INCLUDE_DIR "@PACKAGE_CMAKE_INSTALL_PREFIX@/@CMAKE_INSTALL_INCLUDEDIR@")

include("${CMAKE_CURRENT_LIST_DIR}/OpenSubdivTargets.cmake")


set(OPENSUBDIV_FOUND TRUE)

if(NOT OPENSUBDIV_OSDCPU_LIBRARY)
    if(TARGET OpenSubdiv::osdCPU_static)
      set(OPENSUBDIV_OSDCPU_LIBRARY OpenSubdiv::osdCPU_static)
    elseif(TARGET OpenSubdiv::osdCPU)
      set(OPENSUBDIV_OSDCPU_LIBRARY OpenSubdiv::osdCPU)
    endif()
endif()

if(NOT OPENSUBDIV_OSDGPU_LIBRARY)
    if(TARGET OpenSubdiv::osdGPU_static)
      set(OPENSUBDIV_OSDGPU_LIBRARY OpenSubdiv::osdGPU_static)
    elseif(TARGET OpenSubdiv::osdGPU)
      set(OPENSUBDIV_OSDGPU_LIBRARY OpenSubdiv::osdGPU)
    endif()
endif()

get_target_property(OpenSubdiv_INCLUDE_DIR ${OPENSUBDIV_OSDCPU_LIBRARY} INTERFACE_INCLUDE_DIRECTORIES)


message("Found OpenSubdiv: (found version @OpenSubdiv_VERSION@)")